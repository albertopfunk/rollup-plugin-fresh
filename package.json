{
  "name": "rollup-plugin-fresh",
  "version": "2.3.0",
  "description": "clear targeted files from chosen directory at build time",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "repository": "https://github.com/albertopfunk/rollup-plugin-fresh",
  "bugs": "https://github.com/albertopfunk/rollup-plugin-fresh/issues",
  "author": "albertopfunk",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rollup -c",
    "format": "prettier \"src/**/*.js\" --write",
    "lint": "eslint \"src/**/*.js\" --quiet"
  },
  "keywords": [
    "rollup-plugin",
    "rollup",
    "plugin",
    "clear",
    "remove",
    "delete",
    "clean"
  ],
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "dependencies": {
    "rimraf": "^3.0.0"
  },
  "peerDependencies": {
    "rollup": "> 1.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "prettier": "^1.19.1",
    "rollup": "^1.29.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0"
  }
}
