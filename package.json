{
  "name": "rollup-plugin-fresh",
  "version": "2.6.0",
  "description": "Remove all files or targeted files from a chosen directory at build time",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "repository": "https://github.com/albertopfunk/rollup-plugin-fresh",
  "bugs": "https://github.com/albertopfunk/rollup-plugin-fresh/issues",
  "author": "albertopfunk",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup -c",
    "format": "prettier \"src/**/*.js\" --write",
    "lint": "eslint \"src/**/*.js\" --quiet",
    "prepublish": "npm run format && npm run lint && npm run build"
  },
  "keywords": [
    "rollup-plugin",
    "rollup",
    "plugin",
    "directory",
    "files",
    "clear",
    "remove",
    "delete",
    "clean"
  ],
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "rimraf": "^3.0.2"
  },
  "peerDependencies": {
    "rollup": "> 1.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "prettier": "^1.19.1",
    "rollup": "^1.32.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0"
  }
}
